public class Printer {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String [] words = {"primo", "secondo", "terzo"};

		PrintContext context = new PrintContext();

		context.setLinePrint();
		context.print(words);

		context.setVerticalPrint();
		context.print(words);


	}

}
